@model IEnumerable<InfoMallWeb.Models.ContentImage>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ContentImageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExtraData)
            </th>
            <th>
                Image path
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContentForTabId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContentImageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExtraData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CarImagePath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContentForTabId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ContentImageId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ContentImageId })
            </td>
        </tr>
}
    </tbody>
</table>
